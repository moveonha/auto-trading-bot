    "# ğŸ¯ ê°œì„ ëœ ëª©í‘œ ë³€ìˆ˜ ìƒì„±\n",
    "def create_improved_target_variable(df, lookforward=5, threshold=0.002):  # 0.2% ì„ê³„ê°’\n",
    "    # ë¯¸ë˜ ê°€ê²© ë³€í™”ìœ¨ ê³„ì‚°\n",
    "    df['future_return'] = df['close'].shift(-lookforward) / df['close'] - 1\n",
    "    \n",
    "    # ëª©í‘œ ë³€ìˆ˜ ìƒì„± (ë” ê· í˜•ì¡íŒ ë¶„ë¥˜)\n",
    "    df['target'] = np.where(df['future_return'] > threshold, 1,\n",
    "                      np.where(df['future_return'] < -threshold, -1, 0))\n",
    "    \n",
    "    # ì‹ í˜¸ ê°•ë„ ê³„ì‚°\n",
    "    df['signal_strength'] = abs(df['future_return']) * 100\n",
    "    \n",
    "    return df\n",
    "\n",
    "# ëª©í‘œ ë³€ìˆ˜ ìƒì„±\n",
