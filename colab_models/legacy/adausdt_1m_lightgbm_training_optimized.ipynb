    "# 📊 배치 데이터 수집 (최적화 버전)\n",
    "def collect_data_in_batches():\n",
    "    \"\"\"배치로 데이터 수집\"\"\"\n",
    "    \n",
    "    print('📊 배치 데이터 수집 시작...')\n",
    "    \n",
    "    # 배치 크기 (Supabase 제한 고려)\n",
    "    batch_size = 1000\n",
    "    all_data = []\n",
    "    \n",
    "    # 최신 데이터부터 배치로 수집\n",
    "    offset = 0\n",
    "    total_collected = 0\n",
    "    \n",
    "    while True:\n",
    "        try:\n",
    "            print(f'배치 {offset//batch_size + 1} 수집 중... (offset: {offset})')\n",
    "            \n",
    "            response = supabase.table('crypto_ohlcv').select('*').eq('symbol', 'ADAUSDT').eq('timeframe', '1m').order('timestamp', desc=True).range(offset, offset + batch_size - 1).execute()\n",
    "            \n",
    "            if not response.data:\n",
    "                print('더 이상 데이터가 없습니다.')\n",
    "                break\n",
    "                \n",
    "            batch_data = response.data\n",
    "            all_data.extend(batch_data)\n",
    "            total_collected += len(batch_data)\n",
    "            \n",
    "            print(f'✅ 배치 {offset//batch_size + 1} 완료: {len(batch_data)}개 (총 {total_collected:,}개)')\n",
    "            \n",
    "            # 충분한 데이터를 수집했으면 중단\n",
    "            if total_collected >= 50000:  # 5만개로 제한\n",
    "                print(f'목표 데이터 수집 완료: {total_collected:,}개')\n",
    "                break\n",
    "                \n",
    "            offset += batch_size\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f'❌ 배치 수집 실패: {e}')\n",
    "            break\n",
    "    \n",
    "    if all_data:\n",
    "        df = pd.DataFrame(all_data)\n",
    "        df['datetime'] = pd.to_datetime(df['timestamp'], unit='ms')\n",
    "        df = df.sort_values('datetime')\n",
    "        \n",
    "        print(f'\n✅ 최종 데이터 수집 완료:')\n",
    "        print(f'📊 총 데이터: {len(df):,}개')\n",
    "        print(f'📅 기간: {df["datetime"].min()} ~ {df["datetime"].max()}')\n",
    "        print(f'📈 데이터 크기: {df.shape}')\n",
    "        \n",
    "        return df\n",
    "    else:\n",
    "        print('❌ 데이터 수집 실패')\n",
    "        return None\n",
    "\n",
    "# 데이터 수집 실행\n",
    "df = collect_data_in_batches()"
   ]
  },
  {
   "cell_type": "code",
